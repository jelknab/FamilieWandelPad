<Window x:Class="FamilieWandelPad.RouteBuilder.PoiWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:local="clr-namespace:FamilieWandelPad.RouteBuilder"
        mc:Ignorable="d"
        Title="PoiWindow" Height="450" Width="800">
    <Grid>
        <DockPanel Margin="10">
            <StackPanel DockPanel.Dock="Left">
                <Button Name="btnOpenFile" Click="btnOpenFile_Click">Open file</Button>
                <Image Name="Image" MaxWidth="100"></Image>
            </StackPanel>
            <TabControl Margin="10" Name="Tabs">
                
            </TabControl>
        </DockPanel>
    </Grid>
</Window>
